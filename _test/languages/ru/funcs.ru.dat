<center><code>BLOCK(SETFIELD('Клиент', Null), SETFIELD('Дата', DATE), SETFIELD('Сумма', 0), Null)<br>
BLOCK(SETVAR('MaxDate', MAX('Оплаты', 'Дата')), MAXIF('Оплаты', 'Сумма', '[Дата]=GETVAR("MaxDate")'))</code></center>

